data class Job(val title: String, val salary: Int)

data class Job(val title: String, val salary: Int)

class Person(val name: String) {
    var job: Job? = null

    fun applyForJob(job: Job) {
        // Simulate job application process, could involve random chance or skill checks
        val successChance = 0.8 // Adjust as needed
        if (Math.random() <= successChance) {
            this.job = job
            println("$name got the job: ${job.title}")
        } else {
            println("$name didn't get the job: ${job.title}")
        }
    }
}

fun main() {
    val andre = Person("Andre")
    val cassandra = Person("Cassandra")

    val job1 = Job("Software Engineer", 100000)
    val job2 = Job("Data Scientist", 120000)

    andre.applyForJob(job1)
    cassandra.applyForJob(job2)
}
